<h1>Users</h1>



<ul class="list-group">
    <% @users.each do |user| %>
        <li class="list-group-item"><%= link_to user.username, user %>
          <% if user.suspended  && current_user.admin%>
              <span class="label label-info">account frozen</span>
          <% end %></li>
    <% end %>
</ul>

<br>

<%= link_to 'Sign up', new_user_path,class:"btn btn-primary" %>
